<!--pages/contact/contact.wxml-->
<view class="c-wrap">
	<view class="wechat c-item">
		<button class="cs_button" open-type="contact" 		session-from="weapp">
			<image class="c-icon" src="{{wechatIcon}}" mode="scaleToFill"></image>
		</button> 
		<text class="c-text c-wx">
			微信客服
		</text>
	</view>
	<view class="phone c-item cs_button" bind:tap="calling">
		<image class="c-icon" src="{{phoneIcon}}" mode="scaleToFill"></image>
		<text class="c-text">
			联系客服
		</text>
	</view>
</view>